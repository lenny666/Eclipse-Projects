class Person {
	String name;
	int age;

	void speak() {
			System.out.println("My name is " + name);
	}
	void yearsToRetirement() {
		int yearsLeft = 65 - age;
		
		System.out.println(yearsLeft);
	}
}

public class GetAndReturnValues {

	public static void main(String[] args) {
		Person pets = new Person();
		pets.name = "Peeter";
		pets.age = 29;

		pets.speak();

	}

}
