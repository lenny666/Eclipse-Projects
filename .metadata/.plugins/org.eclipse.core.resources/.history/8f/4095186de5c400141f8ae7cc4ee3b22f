class Person {
	String name;
	int age;

	void speak() {
		for (int i = 0; i < 3; i++) {
			System.out.println("My name is " + name);
		}
	}
}
public class GetAndReturnValues {

	public static void main(String[] args) {
		Person pets = new Person();
		pets.name = "Peeter";
		pets.age = 29;
		
		pets.speak();

	}

}
