import java.util.ArrayList;


public class using_dem_generics {

	public static void main(String[] args) {

		///////////////////// Before Java 5 /////////////////////////////
		ArrayList list = new ArrayList();
		
		list.add("apple");
		list.add("banana");
		list.add("orange");
		
		String fruit = (String) list.get(1);
		
		System.out.println(fruit);
		
	}

}
